#!/usr/bin/env python
# $hgid:

import datetime, re, sys

def to_date(s):
    s = list(map(int, re.split('[:.]', s)))
    return datetime.datetime.now().replace(hour=s[0], minute=s[1])


d1 = to_date(sys.argv[1])

if len(sys.argv) == 2:
    d2 = datetime.datetime.now()
else:
    d2 = to_date(sys.argv[2])

spent = str(d2 - d1).split(':')
spent.pop()
print(':'.join(spent))
